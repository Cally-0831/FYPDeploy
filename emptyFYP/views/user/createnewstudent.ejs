<form action="/user/createnewstudent" method="post" onsubmit="createnewstudent(this);return false;">
  <label>Student name:&nbsp;&nbsp;</label>
  <input type="text" placeholder="Enter Student name" id="studentname" name="studentname" value=""><br>
  <br>
  <label>SID:&nbsp;&nbsp;&nbsp;</label>
  <input type="text" placeholder="Enter SID" id="sid" name="sid" value=""><br><br>
  <label for="topics">Topic:&nbsp;&nbsp;&nbsp;</label>

  <select name="topics" id="topics">
    <% alltopiclist.forEach( function(topic) { %>
      <option value="<%= topic.topic %>">
        <%= topic.topic%>
      </option>
      <% }); %>
  </select>
  <button type="submit is-success">Submit</button>

</form>
<script>

  async function createnewstudent(formElem) {
    try {
      

      var response = await fetch(formElem.action, {
        method: formElem.method,
        body: new FormData(formElem),
      });
      if (response.ok) {
        alert("student created");
        location.assign("/user/liststudent");

      } else if (response.status == 401) {
        var msg = await response.json();
        alert(msg);
      } else {
        alert(response.statusTest);
      }

    } catch (error) {
      console.log(error);
    }
  }


</script>